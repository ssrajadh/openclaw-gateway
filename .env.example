# OpenClaw worker URL (use tunnel if worker is local, e.g. ngrok http 18789)
OPENCLAW_WORKER_URL=http://127.0.0.1:18789
# Bearer token for worker /tools/invoke (match gateway.auth.token or OPENCLAW_GATEWAY_TOKEN)
OPENCLAW_WORKER_TOKEN=your_token_here
# OpenAI API key (required for plan node; used by LangChain ChatOpenAI)
OPENAI_API_KEY=sk-your-openai-key-here
# Port for this FastAPI gateway (optional, default 8000)
PORT=8000
